{
    "setupFiles": ["<rootDir>/src/setupTests.js"],
    "testRegex": "/*.spec.tsx$",
    "collectCoverage": true,
      "collectCoverageFrom": [
        "src/**/*.{js,jsx,ts,tsx}",
        "!<rootDir>/node_modules/",
        "!<rootDir>/path/to/dir/"
      ],
    "coverageReporters": ["lcov"],
    "coverageDirectory": "../coverage/",
    "coverageThreshold": {
     "global": {
     "branches": 0,
     "functions": 0,
     "lines": 0,
     "statements": 0
     }
    },
    "moduleDirectories": ["node_modules", "src"]
   }